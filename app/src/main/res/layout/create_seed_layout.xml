<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <EditText
        android:id="@+id/create_seed_layout_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="TITLE"/>

    <EditText
        android:id="@+id/create_seed_layout_url"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="URL"/>
    <EditText
        android:id="@+id/create_seed_layout_key1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Key 1"/>
    <EditText
        android:id="@+id/create_seed_layout_key2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Key 2"/>

    <EditText
        android:id="@+id/create_seed_layout_key3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Key 3"/>


</LinearLayout>